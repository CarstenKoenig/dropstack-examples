{
  "private": true,
  "name": "npm-lambda-pouch-example",
  "version": "1.0.0",
  "description": "DROPSTACK Lambda-Pouch example",
  "scripts": {
    "register": "lambda-pouch --function example.js && lambda-pouch --function randomize-example.js && lambda-pouch --function failure-example.js",
    "dev": "concurrently \"npm run register\" \"nodemon lambda-pouch\"",
    "start": "npm run register && lambda-pouch",
    "deploy": "dropstack deploy -v SERVICE_PORT=80"
  },
  "author": "DROPSTACK <go@dropstack.run>",
  "license": "MIT",
  "dependencies": {
    "lambda-pouch": "^1.0.3"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "dropstack-cli": "^1.3.2",
    "nodemon": "^1.11.0"
  }
}
